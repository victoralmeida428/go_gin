    "components": {
        "securitySchemes": {
            "BearerAuth": {
                "type": "apiKey",
                "name": "Authorization",
                "in": "header"
            }
        },
        "schemas": {
            "AddGrupamentoInput": {
                "type": "object",
                "required": [
                    "groups_id",
                    "user_id"
                ],
                "properties": {
                    "groups_id": {
                        "type": "integer"
                    },
                    "user_id": {
                        "type": "array",
                        "items": {
                            "type": "integer"
                        }
                    }
                }
            },
            "DelGrupamentoInput": {
                "type": "object",
                "required": [
                    "groups_id",
                    "user_id"
                ],
                "properties": {
                    "groups_id": {
                        "type": "integer"
                    },
                    "user_id": {
                        "type": "integer"
                    }
                }
            },
            "createResponse": {
                "type": "object",
                "required": [
                    "message"
                ],
                "properties": {
                    "message": {
                        "type": "string"
                    }
                }
            },
            "loginRequest": {
                "type": "object",
                "required": [
                    "password",
                    "username"
                ],
                "properties": {
                    "password": {
                        "type": "string"
                    },
                    "username": {
                        "type": "string"
                    }
                }
            },
            "Agendamento": {
                "type": "object",
                "properties": {
                    "active": {
                        "type": "boolean"
                    },
                    "id": {
                        "type": "integer"
                    },
                    "method": {
                        "$ref": "#/components/schemas/Metodo"
                    },
                    "next": {
                        "type": "string"
                    },
                    "periodicity": {
                        "$ref": "#/components/schemas/Periodicidade"
                    },
                    "start": {
                        "type": "string"
                    },
                    "user": {
                        "$ref": "#/components/schemas/User"
                    },
                    "version_form": {
                        "$ref": "#/components/schemas/VersaoFormulario"
                    }
                }
            },
            "Empresa": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "level": {
                        "$ref": "#/components/schemas/Nivel"
                    },
                    "name": {
                        "type": "string"
                    }
                }
            },
            "Formulario": {
                "type": "object",
                "required": [
                    "active",
                    "name"
                ],
                "properties": {
                    "active": {
                        "type": "boolean"
                    },
                    "description": {
                        "type": "string"
                    },
                    "id": {
                        "type": "integer"
                    },
                    "name": {
                        "type": "string"
                    }
                }
            },
            "Grupamento": {
                "type": "object",
                "required": [
                    "name"
                ],
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "name": {
                        "type": "string"
                    }
                }
            },
            "Metodo": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "name": {
                        "type": "string"
                    }
                }
            },
            "Nivel": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "nome": {
                        "type": "string"
                    }
                }
            },
            "Periodicidade": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "interval": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    }
                }
            },
            "TipoVariavel": {
                "type": "object",
                "required": [
                    "nome"
                ],
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "nome": {
                        "type": "string"
                    }
                }
            },
            "User": {
                "type": "object",
                "required": [
                    "first_name",
                    "last_name",
                    "password",
                    "user"
                ],
                "properties": {
                    "email": {
                        "type": "string"
                    },
                    "enterprise": {
                        "$ref": "#/components/schemas/Empresa"
                    },
                    "first_name": {
                        "type": "string"
                    },
                    "groups": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Grupamento"
                        }
                    },
                    "id": {
                        "type": "integer"
                    },
                    "last_name": {
                        "type": "string"
                    },
                    "manager_id": {
                        "type": "integer"
                    },
                    "password": {
                        "type": "string"
                    },
                    "user": {
                        "type": "string"
                    }
                }
            },
            "Variavel": {
                "type": "object",
                "required": [
                    "data_type_id",
                    "text"
                ],
                "properties": {
                    "data_type_id": {
                        "$ref": "#/components/schemas/TipoVariavel"
                    },
                    "has_item": {
                        "type": "boolean"
                    },
                    "id": {
                        "type": "integer"
                    },
                    "question_id": {
                        "type": "integer"
                    },
                    "required": {
                        "type": "boolean"
                    },
                    "text": {
                        "type": "string"
                    }
                }
            },
            "VersaoFormulario": {
                "type": "object",
                "required": [
                    "version"
                ],
                "properties": {
                    "created_at": {
                        "type": "string"
                    },
                    "form": {
                        "$ref": "#/components/schemas/Formulario"
                    },
                    "id": {
                        "type": "integer"
                    },
                    "version": {
                        "type": "integer"
                    }
                }
            }
        }
    }
}