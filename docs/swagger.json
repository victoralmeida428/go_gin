                    "user"
                ],
                "summary": "Create user",
                "parameters": [
                    {
                        "description": "Usuário",
                        "name": "user",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "$ref": "#/components/schemas/User"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/createResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
